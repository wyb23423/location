    <javaModelGenerator targetPackage="com.pos.inhouse.web.dao.model" targetProject="Pos-Inhouse" />
    <sqlMapGenerator targetPackage="mapper" targetProject="Pos-Inhouse/src/main/resources" />
    <javaClientGenerator targetPackage="com.pos.inhouse.web.dao.mapper" targetProject="Pos-Inhouse" type="XMLMAPPER" />

	<table schema="pos" tableName="pos_base_station" enableCountByExample="true" enableDeleteByExample="true" enableSelectByExample="true"  enableUpdateByExample="true" >
		<generatedKey column="id" sqlStatement="MySql" identity="true"/>
